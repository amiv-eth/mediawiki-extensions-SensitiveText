{
    "name": "SensitiveText",
    "version": "0.1.0",
    "author": [
        "Sandro Lutz"
    ],
    "url": "https://gitlab.ethz.ch/amiv/mediawiki-extensions-SensitiveText",
    "descriptionmsg": "sensitivetext-desc",
    "license-name": "AGPLv3",
    "type": "other",
    "requires": {
        "MediaWiki": ">= 1.27.0"
    },
    "MessagesDirs": {
        "SensitiveText": [
            "i18n"
        ]
    },
    "AutoloadClasses": {
        "SensitiveText": "SensitiveText.class.php"
    },
    "Hooks": {
        "ParserFirstCallInit": "SensitiveText::onParserSetup"
    },
    "ResourceModules": {
        "ext.SensitiveText": {
			"scripts": "ext.SensitiveText.js",
			"styles": "ext.SensitiveText.css",
            "messages": [
                "sensitivetext-label"
            ],
        }
    },
    "ResourceFileModulePaths": {
        "localBasePath": "modules",
        "remoteExtPath": "SensitiveText/modules"
    },
    "manifest_version": 1
}
